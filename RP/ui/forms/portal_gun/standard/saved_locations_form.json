{
    "namespace": "standard_saved_locations_form",
    "custom_button": {
        "$default_button_background_texture|default": "textures/ui/presets/red_ore-ui_style/glass_pane",
        "$hover_button_background_texture|default": "textures/ui/presets/red_ore-ui_style/glass_pane_hover",
        "$pressed_button_background_texture|default": "textures/ui/presets/red_ore-ui_style/button_black_hover",
        "$button_size|default": [64, 64],
        "$button_offset|default": [0, 0],
        "$icon_size|default": [45, 45],
        "$icon_offset|default": [0, -5],
        "$text_offset|default": [0, -8],
        "$font_size|default": 1,
        "$font_type|default": "MinecraftRegular",
        "$shadow|default": false,
        "$text_alignment|default": "left",
        "$show_hover_text|default": true,
        "type": "panel",
        "size": "$button_size",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "controls": [
        {
            "panel_name": {
            "type": "panel",
            "size": "$button_size",
            "offset": "$button_offset",
            "anchor_from": "top_left",
            "anchor_to": "top_left",
            "bindings": [
                {
                "binding_type": "view",
                "source_control_name": "image",
                "resolve_sibling_scope": true,
                "source_property_name": "(not (#texture = ''))",
                "target_property_name": "#visible"
                }
            ],
            "controls": [
                {
                "image": {
                    "anchor_from": "top_left",
                    "anchor_to": "top_left",
                    "type": "image",
                    "layer": 200,
                    "size": "$icon_size",
                    "offset": "$icon_offset",
                    "bindings": [
                    {
                        "binding_name": "#form_button_texture",
                        "binding_name_override": "#texture",
                        "binding_type": "collection",
                        "binding_collection_name": "form_buttons"
                    },
                    {
                        "binding_name": "#form_button_texture_file_system",
                        "binding_name_override": "#texture_file_system",
                        "binding_type": "collection",
                        "binding_collection_name": "form_buttons"
                    },
                    {
                        "binding_type": "view",
                        "source_property_name": "(not ((#texture = '') or (#texture = 'loading')))",
                        "target_property_name": "#visible"
                    }
                    ]
                }
                },
                {
                "text": {
                    "anchor_from": "top_left",
                    "anchor_to": "top_left",
                    "type": "label",
                    "text": "#form_button_text",
                    "font_type": "$font_type",
                    "font_scale_factor": "$font_size",
                    "layer": 5,
                    "text_alignment": "$text_alignment",
                    "shadow": "$shadow",
                    "offset": "$text_offset",
                    "bindings": [
                    {
                        "binding_name": "#form_button_text",
                        "binding_type": "collection",
                        "binding_collection_name": "form_buttons"
                    }
                    ]
                }
                }
            ]
            }
        },
        {
            "form_button@common_buttons.light_content_button": {
            "$default_button_texture": "$default_button_background_texture",
            "$hover_button_texture": "$hover_button_background_texture",
            "$pressed_button_texture": "$pressed_button_background_texture",
            "$default_state_border_visible": false,
            "$hover_state_border_visible": false,
            "$pressed_state_border_visible": false,
            "$pressed_button_name": "button.form_button_click",
            "offset": "$button_offset",
            "anchor_from": "top_left",
            "anchor_to": "top_left",
            "size": "$button_size",
            "$button_text": "#null",
            "$button_text_binding_type": "collection",
            "$button_text_grid_collection_name": "form_buttons",
            "$button_text_max_size": ["100%", 20],
            "variables": [
                {
                "requires": "($show_hover_text)",
                "$button_content": "default_namespace.hover_text_panel"
                }
            ],
            "bindings": [
                {
                "binding_type": "collection_details",
                "binding_collection_name": "form_buttons"
                }
            ]
            }
        }
        ],
        "bindings": [
        {
            "binding_name": "#form_button_text",
            "binding_type": "collection",
            "binding_collection_name": "form_buttons"
        },
        {
            "binding_type": "view",
            "source_property_name": "(not (#form_button_text = '') )",
            "target_property_name": "#visible"
        }
        ]
    },

    "pg-skip": {
        "type": "panel",
        "size": [
        "100%",
        "100%c"
        ],
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "controls": [
            {
                "header-collection_panel": {
                    "offset": [0, 0],
                    "size": ["100%", 25],
                    "layer": 6,
                    "type": "collection_panel",
                    "anchor_from": "top_left",
                    "anchor_to": "top_left",
                    "collection_name": "form_buttons",
                    "bindings": [],
                    "controls": [
                        {
                            "back_button@standard_saved_locations_form.custom_button": {
                                "$default_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_default_button",
                                "$hover_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_hover_button",
                                "$pressed_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_pressed_button",
                                "$button_offset": [0, 0],
                                "$button_size": ["100%", "100%"],
                                "layer": 6,
                                "anchor_from": "top_left",
                                "anchor_to": "top_left",
                                "collection_index": 0,
                                "$icon_offset": [0, 0],
                                "$icon_size": [20, 20],
                                "bindings": []
                            }
                        }
                    ]
                }            
            },
            {
                "main_collection_panel": {
                    "offset": [0, 30],
                    "size": ["100%", 25],
                    "layer": 6,
                    "type": "stack_panel",
                    "orientation": "vertical",
                    "anchor_from": "top_left",
                    "anchor_to": "top_left",
                    "collection_name": "form_buttons",
                    "bindings": [],
                    "controls": [
                        {
                            "back_button@standard_saved_locations_form.custom_button": {
                                "$default_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_default_button",
                                "$hover_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_hover_button",
                                "$pressed_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_pressed_button",
                                "$button_offset": [0, 0],
                                "$button_size": ["100%", "100%"],
                                "layer": 6,
                                "anchor_from": "top_left",
                                "anchor_to": "top_left",
                                "collection_index": 0,
                                "$icon_offset": [0, 0],
                                "$icon_size": [20, 20],
                                "bindings": []
                            }
                        },
                        {
                            "back_button1@standard_saved_locations_form.custom_button": {
                                "$default_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_default_button",
                                "$hover_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_hover_button",
                                "$pressed_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_pressed_button",
                                "$button_offset": [0, 0],
                                "$button_size": ["100%", "100%"],
                                "layer": 6,
                                "anchor_from": "top_left",
                                "anchor_to": "top_left",
                                "collection_index": 1,
                                "$icon_offset": [0, 0],
                                "$icon_size": [20, 20],
                                "bindings": []
                            }
                        },
                        {
                            "back_button2@standard_saved_locations_form.custom_button": {
                                "$default_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_default_button",
                                "$hover_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_hover_button",
                                "$pressed_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_pressed_button",
                                "$button_offset": [0, 0],
                                "$button_size": ["100%", "100%"],
                                "layer": 6,
                                "anchor_from": "top_left",
                                "anchor_to": "top_left",
                                "collection_index": 2,
                                "$icon_offset": [0, 0],
                                "$icon_size": [20, 20],
                                "bindings": []
                            }
                        },
                        {
                            "back_button3@standard_saved_locations_form.custom_button": {
                                "$default_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_default_button",
                                "$hover_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_hover_button",
                                "$pressed_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_pressed_button",
                                "$button_offset": [0, 0],
                                "$button_size": ["100%", "100%"],
                                "layer": 6,
                                "anchor_from": "top_left",
                                "anchor_to": "top_left",
                                "collection_index": 3,
                                "$icon_offset": [0, 0],
                                "$icon_size": [20, 20],
                                "bindings": []
                            }
                        }
                    ]                   
                }
            }
        ]
    },

    "pg_form": {
        "offset": [0, 0],
        "size": ["100%", "100%"],
        "layer": 0,
        "type": "panel",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "bindings": [],
        "controls": [
            {
                "bg-image": {
                    "offset": [0, 0],
                    "size": [640, 640],
                    "layer": 2,
                    "type": "image",
                    "texture": "textures/ui/standard_pg_ui/custom_back",
                    "anchor_from": "center",
                    "anchor_to": "center",
                    "bindings": [],
                    "controls": [
                        {
                            "pg-scrolling_panel": {
                                "offset": [0, -15],
                                "size": [220, 110],
                                "type": "panel",
                                "layer": 4,
                                "anchor_to": "center",
                                "anchor_from": "center",
                                "controls": [
                                    {
                                        "pg-sc_linker_panel@common.scrolling_panel": {
                                            "anchor_to": "top_left",
                                            "anchor_from": "top_left",
                                            "$show_background": true,
                                            "size": ["100%", "100%"],
                                            "$scrolling_content": "standard_saved_locations_form.pg-skip",
                                            "$scroll_size": [4, 110],
                                            "$scrolling_pane_size": ["100%", "100%"],
                                            "$scrolling_pane_offset": [0, 0],
                                            "$scroll_bar_right_padding_size": [2, 0],
                                            "bindings": []
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    }
}