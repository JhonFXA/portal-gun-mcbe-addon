{
  "namespace": "prototype_menu_form",

  "custom_button@standard_menu_form.custom_button": {
    "$default_button_background_texture": "textures/ui/presets/green_ore-ui_style/green_default_button",
    "$hover_button_background_texture": "textures/ui/presets/green_ore-ui_style/green_hover_button",
    "$pressed_button_background_texture": "textures/ui/presets/green_ore-ui_style/green_hover_button"
  },

  "menu": {
    "offset": [0, 0],
    "size": ["100%", "100%"],
    "layer": 0,
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "bindings": [],
    "controls": [
      {
        "bg-image": {
          "offset": [0, 0],
          "size": [650, 650],
          "layer": 2,
          "type": "image",
          "texture": "#texture",
          "anchor_from": "center",
          "anchor_to": "center",
          "bindings": [
            {
              "binding_name": "#form_text"
            },
            {
              "binding_type": "view",
              "source_property_name": "((%.100s * (#form_text - (%.0s * #form_text))) - '\t')",
              "target_property_name": "#texture"
            }
          ],
          "controls": [
            {
              "menu-collection_panel": {
                "offset": [-5, 0],
                "size": [250, 110],
                "layer": 4,
                "type": "collection_panel",
                "anchor_from": "center",
                "anchor_to": "center",
                "collection_name": "form_buttons",
                "bindings": [],
                "controls": [
                  {
                    "saved-locations-button@prototype_menu_form.custom_button": {
                      "$button_offset": [35, 10],
                      "$button_size": [85, 90],
                      "layer": 6,
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "collection_index": 0,
                      "$icon_offset": [3.5, 1],
                      "$icon_size": [80, 80],
                      "$font_size": 0.9,
                      "$text_offset": [7.4, 75],
                      "$font_type": "MinecraftRegular",
                      "$shadow": true,
                      "$text_alignment": "center",
                      "$show_hover_text": false,
                      "bindings": []
                    }
                  },
                  {
                    "set-coordinates-button@prototype_menu_form.custom_button": {
                      "$button_offset": [120, 10],
                      "$button_size": [100, 30],
                      "layer": 6,
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "collection_index": 1,
                      "$icon_offset": [3.5, 3],
                      "$icon_size": [20, 20],
                      "$font_size": 0.9,
                      "$text_offset": [25, 10],
                      "$font_type": "MinecraftRegular",
                      "$shadow": true,
                      "$text_alignment": "center",
                      "$show_hover_text": false,
                      "bindings": []
                    }
                  },
                  {
                    "select-mode-button@prototype_menu_form.custom_button": {
                      "$button_offset": [120, 40],
                      "$button_size": [100, 30],
                      "layer": 6,
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "collection_index": 2,
                      "$font_size": 0.9,
                      "$icon_offset": [3.5, 1],
                      "$icon_size": [20, 25],
                      "$text_offset": [30, 10],
                      "$font_type": "MinecraftRegular",
                      "$shadow": true,
                      "$text_alignment": "center",
                      "$show_hover_text": false,
                      "bindings": []
                    }
                  },
                  {
                    "settings-button@prototype_menu_form.custom_button": {
                      "$button_offset": [120, 70],
                      "$button_size": [30, 30],
                      "layer": 6,
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "collection_index": 3,
                      "$icon_offset": [4.7, 3.2],
                      "$icon_size": [20, 20],
                      "bindings": []
                    }
                  },
                  {
                    "info_label": {
                      "type": "label",
                      "font_type": "MinecraftTen",
                      "font_size": "medium",
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "text": "#info_text",
                      "layer": 8,
                      "offset": [150, 100],
                      "bindings": [
                        { "binding_name": "#form_text" },
                        {
                          "binding_type": "view",
                          "source_property_name": "(#form_text - (%.100s * #form_text))",
                          "target_property_name": "#info_text"
                        }
                      ]
                    }
                  },
                  {
                    "close-button@prototype_menu_form.custom_button": {
                      "$default_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_default_button",
                      "$hover_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_hover_button",
                      "$pressed_button_background_texture": "textures/ui/presets/red_ore-ui_style/red_pressed_button",
                      "$button_offset": [70, -10],
                      "$button_size": [30, 30],
                      "layer": 6,
                      "anchor_from": "top_right",
                      "anchor_to": "top_right",
                      "collection_index": 4,
                      "$icon_offset": [4.7, 3.2],
                      "$icon_size": [20, 20],
                      "bindings": []
                    }
                  },
                  {
                    "unplug-tube-button@prototype_menu_form.custom_button": {
                      "$button_offset": [70, -40],
                      "$button_size": [30, 30],
                      "layer": 6,
                      "anchor_from": "top_right",
                      "anchor_to": "top_right",
                      "collection_index": 5,
                      "$icon_offset": [4.7, 3.2],
                      "$icon_size": [20, 20],
                      "bindings": []
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  }
}
